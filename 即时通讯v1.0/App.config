<?xml version="1.0" encoding="utf-8" ?>
<configuration>
      <appSettings>
        <!--服务器IP -->
        <add key="ServerIP" value="127.0.0.1"/>
        <!--服务器端口-->
        <add key="ServerPort" value="9800"/>
        <!--使用的扬声器的索引-->
        <add key="SpeakerIndex" value="0"/>
        <!--使用的麦克风的索引-->
        <add key="MicrophoneIndex" value="0"/>
        <!--服务器监听的端口-->
        <add key="Port" value="9800"/>
        <!--OMCS授权用户的账号-->
        <add key="AuthorizedUser" value="FreeUser"/>
        <!--OMCS授权用户的密码-->
        <add key="AuthorizedPassword" value=""/>
        <!--是否记录安全日志文件 Omcs.txt-->
        <add key="SecurityLogEnabled" value="false"/>
        <!--UserID的最大长度-->
        <add key="MaxLengthOfUserID" value="11"/>
      </appSettings> 
</configuration>